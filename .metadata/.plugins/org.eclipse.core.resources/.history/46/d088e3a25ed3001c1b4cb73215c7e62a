package ch04;

public class Student {
	
	// 생성자 생성 예제
	
	public int studentNumber;
	public String studentName;
	public int grade;
	
	public Student() {
		System.out.println("student 생성");
	}
	public String showStudentInfo() {
		
		return studentName + "학생의 학번은 " + studentNumber + "이고, " + grade + "학년 입니다.";
	}

}
