package ch11;

public class ArrayTest {

	public static void main(String[] args) {
		
		// 배열로 합을 구하는 예제

		int[] arr = new int[10];
		int total = 0;
		
		for(int i = 0, num = 1; i < arr.length; i++) {
			arr[i] = num++;
		}
		for(int num : arr) { // 배열의 처음부터 배열의 끝까지 쭉 순회 할 때 사용
			total += num;
		}
		
		
	}

}
